<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

  <configuration>
    <stop-on-errors>true</stop-on-errors>
    <date-format>MM/dd/yyyy HH:mm</date-format>
    <wildcard>true</wildcard>
  </configuration>

  <mapping>
    <class-a>com.hg.dozer.vo.TestObjectFull</class-a>
    <class-b>com.hg.dozer.vo.TestObjectPart</class-b>
      <field>
        <a>id</a>
        <b>bianhao</b>
      </field>
       <field>
        <a>name</a>
        <b>xingming</b>
      </field>
  </mapping> 
  
  
  <mapping wildcard="false">
    <class-a>com.hg.dozer.vo.A</class-a>
    <class-b>com.hg.dozer.vo.C</class-b>
    <field>
       <a>a</a>
       <b>a</b>
    </field>   
    <field>
       <a>b</a>
       <b>b</b>
    </field>   
</mapping>

<mapping wildcard="false">
    <class-a>com.hg.dozer.vo.B</class-a>
    <class-b>com.hg.dozer.vo.C</class-b>
    <field>
       <a>c</a>
       <b>c</b>
    </field> 
</mapping>
            
</mappings>  